{
  "plugins": [
    "node_modules/jsdoc-plugin-typescript"
  ],
  "source": {
    "include": [
      "src/app"
    ],
    "includePattern": ".+\\.(ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./out/docs",
    "recurse": true,
    "readme": "./README.md"
  },
  "typescript": {
    "moduleRoot": "src/app",
    "check": false,
    "outDir": "./out",
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "templates": {
    "default": {
      "includeDate": false
    }
  }
}
