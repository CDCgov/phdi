{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "CarePlan",
        "id":"{{ID}}",
        "identifier":
        [
            {% assign ids = carePlan.id | to_array -%}
            {% for id in ids -%}
                { {% include 'DataType/Identifier' Identifier: id -%} }
            {% endfor -%}
        ],
        "status": "change me",
        "intent": "change me",
        "subject": "reference to patient",
        "activity":[
            {% for carePlanEntry in carePlan.text.table.tbody.tr %}
            {
                "detail": {
                    "code":{ {% include 'DataType/CodeableConcept' Text: carePlanEntry.td[0]._ -%} },
                    "status":"scheduled",
                    "scheduledString": "{{ carePlanEntry.td[2]._ }}",
                },
                "extension": [
                    {
                        "url": "dibbs.orderedDate",
                        "valueString": "{{ carePlanEntry.td[1]._ }}"
                    }
                ]
            },
            {% endfor %}
        ]
    },
    "request":{
        "method":"PUT",
        "url":"CarePlan/{{ID}}",
    },
},